#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int main()

{
    int n,i ,m=0, flag=0 ; 
    
    cout<<"\n Enter any number =  ";
    cin>>n;
    
    m=n/2;
    
    for(int i=2;i<m;i++)
    {
        if(n%i==0)
        {
            cout<<"\n The number is not prime.";
            flag=1;
            break;
        }
    }
    if(flag==0)
    {
        cout<<"\n The number is prime.";
    }
    return 0;
}
